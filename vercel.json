{
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "framework": "nextjs",
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api/:path*"
    }
  ],
  "env": {
    "NEXT_PUBLIC_BACKEND_URL": "https://your-backend-url.vercel.app",
    "NEXT_PUBLIC_FRONTEND_URL": "https://document-a11y-acclerator.vercel.app/home"
  }
}
